{
  "name": "artstation-scraping",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server.js",
    "start:worker": "node ./build/worker/index.js",
    "dev:server": "node -r ts-node/register ./src/server.ts",
    "dev:worker": "node -r ts-node/register ./src/worker/index.ts",
    "dev:debug": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "dev:watch": "nodemon",
    "dev:worker:watch": "nodemon --config nodemon-worker.json",
    "build-start": "npm run build && node ./build/index.js",
    "build": "npm run tslint && tsc",
    "lint": "tslint --project tsconfig.json",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^3.1.1",
    "bull": "^3.13.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "google-auth-library": "^5.9.2",
    "googleapis": "^39.2.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.3",
    "open": "^7.0.2",
    "puppeteer": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "server-destroy": "^1.0.1",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.0.0",
    "@types/bull": "^3.12.1",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.3",
    "@types/puppeteer": "^2.0.0",
    "@types/request": "^2.48.4",
    "@types/throng": "^4.0.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "src/worker/*"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev:server",
    "ext": "ts"
  }
}
